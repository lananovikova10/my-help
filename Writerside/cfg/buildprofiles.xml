<?xml version="1.0" encoding="UTF-8"?>
<buildprofiles xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <variables></variables>
    <build-profile instance="b">
        <variables>
            <noindex-content>false</noindex-content>
            <offline-docs>false</offline-docs>
            <web-root>https://some.domain</web-root>
            <showDownloadButton>true</showDownloadButton>
            <download-title>One text</download-title>
            <download-page>https://google.com</download-page>
            <primary-color>#DEB7FF</primary-color>
            <enable-browser-edits>true</enable-browser-edits>
            <browser-edits-url>https://github.com/lananovikova10/my-help/tree/main/Writerside/</browser-edits-url>
        </variables>
    </build-profile>
    <build-profile instance="m">
        <variables>
            <showDownloadButton>true</showDownloadButton>
            <download-title>Another text</download-title>
            <download-page>https://google.com</download-page>
        </variables>
    </build-profile>
    <icons><local-src>/icons</local-src></icons>

</buildprofiles>
